{
    "family": "inventory-task",
    "executionRoleArn": "arn:aws:iam::753829278316:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "inventory-app",
            "image": "<IMAGE_PLACEHOLDER>",
            "secrets": [
                {
                    "name": "MONGO_URI",
                    "valueFrom": "/alegra/MONGO_URI"
                }
            ],
            "environment": [
                {
                    "name": "PURCHASE_URL",
                    "value": "/alegra/PURCHASE_URL"
                }
            ],
            "memory": 512,
            "cpu": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3002,
                    "hostPort": 3002
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "inventory-app",
                    "awslogs-region": "TO_BE_REPLACED",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}