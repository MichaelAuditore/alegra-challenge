{
    "family": "orders-task",
    "executionRoleArn": "arn:aws:iam::140023362106:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "orders-app",
            "image": "<IMAGE_PLACEHOLDER>",
            "secrets": [
                {
                    "name": "REDIS_URL",
                    "valueFrom": "/alegra/REDIS_URL"
                },
                {
                    "name": "PGSQL_DATABASE_URL",
                    "valueFrom": "/alegra/PGSQL_DATABASE_URL"
                }
            ],
            "memory": 128,
            "cpu": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 0
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "orders-app",
                    "awslogs-region": "TO_BE_REPLACED",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}