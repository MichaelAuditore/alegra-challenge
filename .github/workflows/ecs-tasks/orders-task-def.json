{
    "family": "orders-task",
    "executionRoleArn": "arn:aws:iam::140023362106:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "orders-app",
            "image": "<IMAGE_PLACEHOLDER>",
            "secrets": [
                {
                    "name": "REDIS_URL",
                    "valueFrom": "/alegra/REDIS_URL"
                },
                {
                    "name": "PGSQL_DATABASE_URL",
                    "valueFrom": "/alegra/PGSQL_DATABASE_URL"
                }
            ],
            "memory": 512,
            "cpu": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "placementConstraints": [
        {
            "type": "memberOf",
            "expression": "attribute:ecs.instance-id == i-074a53a5dc37e934a"
        }
    ]
}